from typing import List
class Solution:
    def subarraySum(self, nums: List[int], k: int) -> int:
        # 2 loop
        count = 0
        for i in range(len(nums)):
            current_sum = 0
            for j in range(i, len(nums)):
                current_sum += nums[j]
                if current_sum == k:
                    count += 1
        return count
    
sol = Solution()
print(sol.subarraySum([1,1,1], 2))  # Output: 2
print(sol.subarraySum([1,2,3], 3))  # Output: 2 
