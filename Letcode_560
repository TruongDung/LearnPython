class Solution:
    def subarraySum(self, nums: List[int], k: int) -> int:
        
        count = 0
        prefix_sum = 0
        prefix_sums = {0: 1}

        for num in nums:
            prefix_sum += num
            if (prefix_sum - k) in prefix_sums:
                count += prefix_sums[prefix_sum - k]
            prefix_sums[prefix_sum] = prefix_sums.get(prefix_sum, 0) + 1

        return count
    
sol = Solution()
print(sol.subarraySum([1,1,1], 2))  # Output: 2
print(sol.subarraySum([1,2,3], 3))  # Output: